<!DOCTYPE html>
<html>
  <head>
    <title>FurnitureSite</title>
    <%= csrf_meta_tags %>
    <!-- bootstrap.min css -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <%= stylesheet_link_tag 'application_admin', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application_admin', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
      <%= render 'admin/partial/header' %>
      <%= render 'admin/partial/sidebar' %>
      <%= yield %>
      <%= render 'admin/partial/footer' %>
    </div>
  </body>
</html>